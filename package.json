{
  "name": "ultimate-gatsby-starter",
  "private": true,
  "description": "A bare-bones typescript starter for Gatsby using Tailwind, Styled Components & Twin.Macros configured for testing",
  "version": "0.1.0",
  "license": "0BSD",
  "author": "LopÃ¨ Ariyo",
  "scripts": {
    "develop:gatsby": "gatsby develop",
    "build:gatsby": "gatsby build",
    "start": "npm run develop:gatsby",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve:gatsby": "gatsby serve",
    "clean:gatsby": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "babelMacros": {
    "twin": {
      "config": "src/tailwind.config.js",
      "preset": "styled-components",
      "autoCssProp": true,
      "hasSuggestions": true
    }
  },
  "dependencies": {
    "babel-plugin-remove-graphql-queries": "^2.13.0",
    "babel-plugin-styled-components": "^1.12.0",
    "framer-motion": "^4.1.17",
    "gatsby": "2.29.1",
    "gatsby-plugin-image": "1.5.0",
    "gatsby-plugin-manifest": "3.5.0",
    "gatsby-plugin-offline": "4.5.0",
    "gatsby-plugin-postcss": "4.5.0",
    "gatsby-plugin-react-helmet": "4.5.0",
    "gatsby-plugin-sharp": "3.5.0",
    "gatsby-plugin-styled-components": "4.5.0",
    "gatsby-plugin-tailwindcss": "^1.0.3",
    "gatsby-plugin-typescript": "^3.5.0",
    "gatsby-source-filesystem": "3.5.0",
    "gatsby-transformer-sharp": "3.5.0",
    "postcss-import": "^14.0.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "styled-components": "^5.3.0",
    "styled-reset-advanced": "^1.0.3",
    "tailwindcss": "^2.1.2",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/addon-storysource": "6.2.9",
    "@storybook/addons": "6.2.9",
    "@storybook/react": "6.2.9",
    "autoprefixer": "^10.2.5",
    "babel-loader": "8.2.2",
    "babel-plugin-root-import": "^6.6.0",
    "babel-preset-gatsby": "1.5.0",
    "babel-preset-react-app": "10.0.0",
    "html-webpack-plugin": "5.3.1",
    "postcss": "^8.3.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.3.0",
    "twin.macro": "^2.4.2",
    "typescript-plugin-tw-template": "^2.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "keywords": [
    "gatsby",
    "starter",
    "typescript",
    "tailwind",
    "styled-components",
    "css-in-jss",
    "storybook"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
